									"// Проверка статуса",
									"pm.test(\"Status code is 200\", function() {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"// Проверка структуры ответа",
									"pm.test(\"Response has courses array\", function() {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.courses).to.be.an('array');",
									"});",
									"",
									"// Проверка структуры первого курса",
									"pm.test(\"First course has correct structure\", function() {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.courses.length > 0) {",
									"        var firstCourse = jsonData.courses[0];",
									"        pm.expect(firstCourse).to.have.property('id');",
									"        pm.expect(firstCourse).to.have.property('name');",
									"        pm.expect(firstCourse).to.have.property('code');",
									"        pm.expect(firstCourse.id).to.be.a('number');",
									"        ",
									"        // Сохраняем ID первого курса",
									"        pm.environment.set(\"course_id\", firstCourse.id.toString());",
									"    }",
									"});",
